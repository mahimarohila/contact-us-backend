    <div class="centers">
      <div class="row bg-primary col-lg-7 container-fluid mb-5">
        <div class="row justify-content-center p-3">
          <h1 class="text-white display-4">Drop Us A Message!</h1>
        </div>
        <br /><br />
        <div class="offset-2">
          <form action="/index" class="pl-5" method="post"><br>
            <div class="mb-3">
                <label for="name" class="text-white form-label">Name</label>
                <input type="text" class="form-control" id="name" name="firstname">
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="text-white form-label">Email address</label>
                <input type="email" class="form-control" id="exampleInputEmail1" name="email"
                    aria-describedby="emailHelp">
                <div id="emailHelp" class="text-white form-text">We'll never share your email with anyone else.
                </div>
            </div>

            <div class="mb-3">
                <label for="comment" class="text-white form-label">Your Comment</label>
                <textarea class="form-control" name="comment" id="ecomment"></textarea>
            </div>
            <div class="mb-3 form-check">
                <input type="checkbox" class="form-check-input" id="exampleCheck1" name="check">
                <label class="text-white form-check-label" for="exampleCheck1">Are You A Member</label>
            </div>

            <input type="submit" value="Submit" class="bg-light btn btn-outline-primary"><br><br><br>
        </form>
        </div>
      </div>
    </div>

